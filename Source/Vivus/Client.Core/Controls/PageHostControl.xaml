<UserControl x:Class="Vivus.Client.Core.Controls.PageHostControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Vivus.Client.Core.Controls"
             xmlns:attachedProperties="clr-namespace:Vivus.Client.Core.AttachedProperties"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             x:Name="PageHost">

    <Grid>
        <Frame x:Name="OldPage" attachedProperties:NoFrameHistory.Value="True" />
        <Frame x:Name="NewPage" attachedProperties:NoFrameHistory.Value="True" />
    </Grid>
</UserControl>
